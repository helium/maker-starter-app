    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "react-native-cli-bump-version": "^1.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "~4.3.5"
  }
}
