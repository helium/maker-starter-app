    "react-native-appstate-hook": "^1.0.5",
    "react-native-ble-plx": "^2.0.3",
    "react-native-config": "^1.4.1",
    "react-native-device-info": "^8.4.4",
    "react-native-gesture-handler": "~1.10.2",
