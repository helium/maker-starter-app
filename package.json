{
  "name": "FreedomFi",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "icon": "yarn icon-label && yarn icon-gen && yarn icon-cleanup",
    "icon-label": "app-icon label -i icon.png -o output.png --bottom 3.0.0",
    "icon-gen": "app-icon generate -i output.png",
    "icon-cleanup": "rm output.png",
    "android-clean": "cd android && ./gradlew clean && rm -rf .gradle rm -rf app/build && cd ..",
    "ios-clean": "rm -rf ios/Pods && rm -rf ios/build",
    "pod-install": "cd ios && pod install && cd ..",
    "clean-install": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules && yarn cache clean && yarn install && yarn android-clean && yarn ios-clean && yarn pod-install",
    "clean-start": "yarn start -- --reset-cache",
    "postinstall": "yarn patch-package",
    "update-version": "npx react-native-version --never-amend",
    "lint": "yarn lint:prettier && yarn lint:package-json && yarn lint:eslint",
    "lint:fix": "yarn lint:prettier:fix && yarn lint:package-json && yarn lint:eslint:fix",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --loglevel warn --check \"**/*.{css,scss,js,jsx,ts,tsx,html,md,json,yml}\"",
    "lint:prettier:fix": "prettier --loglevel warn --write \"**/*.{css,scss,js,jsx,ts,tsx,html,md,json,yml}\"",
    "lint:package-json": "npmPkgJsonLint .",
    "lint:typescript": "tsc --pretty --noEmit",
    "ci": "yarn lint"
  },
